{% extends 'base-admin.html.twig' %}

{% block title %}Résultat Sondage{% endblock %}

{% block body %}
    <h1>Résultat du sondage {{ survey.title }}</h1>
    {% set i = 1 %}
    <div class="row">
        {% for question in survey.questions %}
            <div class="card-result">
                <h5>Question n°{{ i }} : {{ question.title }}</h5>

            </div>
            {% set i = i + 1 %}
        {% endfor %}
    </div>
{% endblock %}

